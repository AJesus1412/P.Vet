      Familia de mascotas
    </div>
  </div>
</div>

<div class="row"> 
  <div class="col-sm-12">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Athome</th>
          <th>Insite</th>
          <th>Lugar</th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>
        <% @service_types.each do |service_type| %>
        <tr>
          <td><%= service_type.atHome %></td>
          <td><%= service_type.inSite %></td>
          <td><%= service_type.lugar %></td>
          <td><%= link_to 'Mostrar', service_type %></td>
          <td><%= link_to 'Editar', edit_service_type_path(service_type) %></td>
          <td><%= link_to 'Eliminar', service_type, method: :delete, data: { confirm: 'EstÃ¡ seguro de eliminar?' } %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
    <br>
    <%= link_to 'Nuevo servicio', new_service_type_path %>
  </div>
</div>